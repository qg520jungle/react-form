{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
        // "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
        // "prettier"
    ],
    "settings": {
        "import/resolver": {
          "node": {
            "paths": ["src"]
          }
        }
    },
    "rules": {
        // "quotes": 1
        // "prettier/prettier": "error"
        // "semi": ["error", "always"],
        // "quotes": [
        //     "error",
        //     "single",
        //     { "avoidEscape": true, "allowTemplateLiterals": false }
        // ]
    }
}
